  <div class="row">
    <div class="col-md-3">
      <%= render 'users/info', users: @users %>
    </div>
  </div>

  <div class="col-md-8 offset-md-1">
    <h2>Users</h2>
    <table class="table">
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <%= @users.each do |user| %>
        <tr>
          <td><%= profile_image_tag(' %></td>
          <td><%= @user.name %></td>
          <td><%= link_to"show", user_path(user.id) %></td>
        </tr>
      </tbody>
    </table>
  </div>